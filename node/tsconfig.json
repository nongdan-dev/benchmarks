{
  "compilerOptions": {
    "target": "ES6",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "types": ["node", "express"] // Thêm dòng này vào
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules"]
}
